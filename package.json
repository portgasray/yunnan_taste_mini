    "@tarojs/plugin-platform-swan": "4.1.1",
    "@tarojs/plugin-platform-tt": "4.1.1",
    "@tarojs/plugin-platform-weapp": "4.1.1",
    "@tarojs/react": "4.1.1",
    "@tarojs/runtime": "4.1.1",
    "@tarojs/shared": "4.1.1",
